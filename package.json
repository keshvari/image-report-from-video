{
  "name": "electron-screen-recorder",
  "description": "Screen Recorder",
  "version": "0.0.3",
  "author": "hokein",
  "license": "BSD",
  "keywords": [
    "Electron",
    "DesktopCapture",
    "Screen Recorder",
    "WebRTC"
  ],
  "scripts": {
    "test": "electron .",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "main": "src/main.js",
  "bin": {
    "electron-screen-recorder": "./launch.js"
  },
  "build": {
    "appId": "reportgenerator",
    "category": "your.app.category.type",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "electron": "^8.0.3",
    "electron-builder": "^22.6.1",
    "electron-packager": "^10.1.2",
    "electron-prebuilt": "^1.4.13"
  },
  "dependencies": {
    "annyang": "^2.6.1",
    "bootstrap": "^4.4.1",
    "canvas": "^2.6.1",
    "domify": "^1.4.0",
    "electron-canvas-to-buffer": "^2.0.0",
    "html2pdf.js": "^0.9.2",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "js-logger": "^1.6.0",
    "jspdf": "^1.5.3",
    "jspdf-autotable": "^3.2.13",
    "lodash": "^4.17.15",
    "multiple-select": "^1.5.2",
    "persianjs": "^0.4.0",
    "popper": "^1.0.1",
    "snowboy": "^1.3.1-alpha.1",
    "sonus": "^1.0.3",
    "webcamjs": "^1.0.26"
  }
}
